import numpy as np
import matplotlib.pyplot as plt

x = np.linspace(-10, 10, 100)

y1 = np.sin(x)
y2 = np.sin(2*x)
y3 = np.sin((1/2)*x)
y4 = 2 * np.sin(x)
y5 = (1/2)*np.sin(x) 


plt.plot(x, y1, 'purple')
plt.plot(x, y2, 'pink')
plt.plot(x, y3, 'green')
plt.plot(x, y4, 'orange')
plt.plot(x, y5, 'blue')
plt.savefig('galaxy')
